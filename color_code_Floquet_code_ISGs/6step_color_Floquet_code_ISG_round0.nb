Cell[BoxData[
 RowBox[{
  RowBox[{"expandEntry", "[", 
   RowBox[{"poly_", ",", "x_", ",", "n_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "f", ",", "p", ",", "c", ",", "xp", ",", "col", ",", "monos", ",", 
      "exps", ",", "gen"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"f", "[", 
       RowBox[{"i_", ",", "j_"}], "]"}], ":=", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"i", "-", "1"}], "\[Equal]", "j"}], ",", "1", ",", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"i", "-", "1", "+", "n"}], "\[Equal]", "j"}], ",", "x", 
          ",", "0"}], "]"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"gen", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"f", "[", 
         RowBox[{"i", ",", "j"}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", "n"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"j", ",", "1", ",", "n"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"p", "=", 
      RowBox[{"Expand", "[", "poly", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"p", "===", "0"}], ",", "\[IndentingNewLine]", 
       RowBox[{"(*", 
        RowBox[{
        "return", " ", "zero", " ", "matrix", " ", "if", " ", "the", " ", 
         "input", " ", "is", " ", "zero"}], "*)"}], "\[IndentingNewLine]", 
       RowBox[{"Table", "[", 
        RowBox[{"0", ",", 
         RowBox[{"{", "n", "}"}], ",", 
         RowBox[{"{", "n", "}"}]}], "]"}], ",", "\[IndentingNewLine]", 
       RowBox[{"(*", 
        RowBox[{"Else", ",", " ", 
         RowBox[{"compute", " ", "properly"}]}], "*)"}], 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"col", "=", 
         RowBox[{"Collect", "[", 
          RowBox[{"p", ",", "x"}], "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"monos", "=", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"Head", "[", "col", "]"}], "===", "Plus"}], ",", 
           RowBox[{"Apply", "[", 
            RowBox[{"List", ",", "col"}], "]"}], ",", 
           RowBox[{"{", "col", "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"exps", "=", 
         RowBox[{"Map", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"Exponent", "[", 
             RowBox[{"#", ",", "x"}], "]"}], "&"}], ",", "monos"}], "]"}]}], 
        ";", "\[IndentingNewLine]", 
        RowBox[{"c", "=", 
         RowBox[{
          RowBox[{"Power", "[", 
           RowBox[{"x", ",", 
            RowBox[{"-", "exps"}]}], "]"}], "*", "monos"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"xp", "=", 
         RowBox[{"Map", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"MatrixPower", "[", 
             RowBox[{"gen", ",", "#"}], "]"}], "&"}], ",", "exps"}], "]"}]}], 
        ";", "\[IndentingNewLine]", 
        RowBox[{"Expand", "[", 
         RowBox[{"c", ".", "xp"}], "]"}]}]}], "\[IndentingNewLine]", 
