      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"i", "-", "1"}], "\[Equal]", "j"}], ",", "1", ",", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
