           RowBox[{"{", "col", "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"exps", "=", 
         RowBox[{"Map", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"Exponent", "[", 
             RowBox[{"#", ",", "x"}], "]"}], "&"}], ",", "monos"}], "]"}]}], 
        ";", "\[IndentingNewLine]", 
        RowBox[{"c", "=", 
         RowBox[{
          RowBox[{"Power", "[", 
           RowBox[{"x", ",", 
