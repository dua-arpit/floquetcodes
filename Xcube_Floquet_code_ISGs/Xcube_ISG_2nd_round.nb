          RowBox[{"Exponent", "[", 
           RowBox[{"f", ",", "#"}], "]"}], "&"}], ",", "v"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"mono", "=", 
       RowBox[{"Apply", "[", 
        RowBox[{"Times", ",", 
         RowBox[{"Power", "[", 
          RowBox[{"v", ",", "e"}], "]"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"c", "=", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"mono", "===", "1"}], ",", "f", ",", 
         RowBox[{"Coefficient", "[", 
          RowBox[{"f", ",", "mono"}], "]"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"c", "*", 
       RowBox[{"Apply", "[", 
        RowBox[{"Times", ",", 
         RowBox[{"Power", "[", 
          RowBox[{"v", ",", 
