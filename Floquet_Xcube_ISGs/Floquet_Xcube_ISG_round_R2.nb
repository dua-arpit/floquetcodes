       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"col", "=", 
         RowBox[{"Collect", "[", 
          RowBox[{"p", ",", "x"}], "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"monos", "=", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"Head", "[", "col", "]"}], "===", "Plus"}], ",", 
           RowBox[{"Apply", "[", 
            RowBox[{"List", ",", "col"}], "]"}], ",", 
           RowBox[{"{", "col", "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"exps", "=", 
         RowBox[{"Map", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"Exponent", "[", 
             RowBox[{"#", ",", "x"}], "]"}], "&"}], ",", "monos"}], "]"}]}], 
        ";", "\[IndentingNewLine]", 
        RowBox[{"c", "=", 
         RowBox[{
          RowBox[{"Power", "[", 
           RowBox[{"x", ",", 
            RowBox[{"-", "exps"}]}], "]"}], "*", "monos"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"xp", "=", 
         RowBox[{"Map", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"MatrixPower", "[", 
             RowBox[{"gen", ",", "#"}], "]"}], "&"}], ",", "exps"}], "]"}]}], 
        ";", "\[IndentingNewLine]", 
        RowBox[{"Expand", "[", 
         RowBox[{"c", ".", "xp"}], "]"}]}]}], "\[IndentingNewLine]", 
