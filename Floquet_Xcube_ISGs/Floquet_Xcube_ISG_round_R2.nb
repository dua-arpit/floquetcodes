Cell[BoxData[
 RowBox[{
  RowBox[{"expandEntry", "[", 
   RowBox[{"poly_", ",", "x_", ",", "n_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
