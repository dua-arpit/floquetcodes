       RowBox[{"Apply", "[", 
        RowBox[{"Times", ",", 
         RowBox[{"Power", "[", 
          RowBox[{"v", ",", "e"}], "]"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"c", "=", 
       RowBox[{"If", "[", 
